  'clingo invaders.lp aliens9.lp --time-limit=300' \
  'clingo invaders.lp aliens10.lp --time-limit=300'

/home/linuxbrew/.linuxbrew/bin/hyperfine --warmup 3 --runs 10 --export-json ./benchmarks/_generated_benchmark_trendy.json --ignore-failure \
  'clingo invaders.lp aliens1.lp --configuration=trendy --time-limit=300' \
