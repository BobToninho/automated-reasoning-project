  'clingo invaders.lp aliens6.lp --configuration=trendy --time-limit=300' \
  'clingo invaders.lp aliens7.lp --configuration=trendy --time-limit=300' \
  'clingo invaders.lp aliens8.lp --configuration=trendy --time-limit=300' \
  'clingo invaders.lp aliens9.lp --configuration=trendy --time-limit=300' \
  'clingo invaders.lp aliens10.lp --configuration=trendy --time-limit=300'

